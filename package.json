{
  "name": "todolist",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/index.js",
      "!src/registerServiceWorker.js"
    ]
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint-plugin-react": "^7.10.0",
    "eslint-watch": "^4.0.2",
    "jest": "20.0.4",
    "jest-cli": "20.0.4",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "prestart": "npm run flow-status",
    "start": "npm-run-all --parallel lint:watch test:watch react-start",
    "react-start": "react-scripts start",
    "build": "react-scripts build",
    "prebuild": "npm-run-all test:coverage lint flow-status",
    "test:coverage":"npm run test -- --coverage",
    "test": "react-scripts test --env=jsdom",
    "test:watch": "npm run test",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint src/**/*.js --color",
    "flow-status": "flow status --color always",
    "lint:watch": "./node_modules/.bin/esw src/**/*.js --color -- --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^5.3.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "flow-bin": "^0.78.0"
  }
}
